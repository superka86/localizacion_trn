<?xml version="1.0" encoding="utf-8"?>
<odoo>

  <record id="action_retention_line" model="ir.actions.act_window">
    <field name="name">Retention Line</field>
    <field name="res_model">retention.line</field>
    <field name="view_mode">tree,form</field>
    <field name="help" type="html">
      <p class="o_view_nocontent_smiling_face">
        No holds have been created yet
      </p>
    </field>
  </record>

  <record id="retention_line_view_tree" model="ir.ui.view">
    <field name="name">retention.line.view.tree</field>
    <field name="model">retention.line</field>
    <field name="arch" type="xml">
      <tree string="Retention Lines">
        <field name="invoice_id" />
      </tree>
    </field>
  </record>

</odoo>
