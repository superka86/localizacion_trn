<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="portal_bdv_payment.payment_acquirer_bdv" model="payment.provider">
            <field name="name">BDV IPG</field>
            <field name="image_128" type="base64" file="portal_bdv_payment/static/src/img/bdv.png"/>
            <field name="code">bdv</field>
            <field name="company_id" ref="base.main_company"/>
            <field name="default_url">/bdv/payment</field>
            <field name="url_to_return">/bdv/process_payment/</field>
        </record>
        <record id="portal_bdv_payment.payment_acquirer_bdv_p2p" model="payment.provider">
            <field name="name">BDV P2P</field>
            <field name="image_128" type="base64" file="portal_bdv_payment/static/src/img/bdv.png"/>
            <field name="code">bdv_p2p</field>
            <field name="company_id" ref="base.main_company"/>
            <field name="default_url">/bdv/p2p/payment</field>
        </record>
    </data>
</odoo>
